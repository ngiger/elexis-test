<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/stylesheets' %>
		<%= javascript_include_tag :defaults %>
	</head>
	<body>
		<div class="container">
			<div>
				<%= render 'layouts/header' %>
			</div>
			<div class="links">
				<b>Main</b>
				<br>
				<%= link_to 'Versions','product_versions' %>
				<br>
				<%= link_to 'Descriptions','product_versions' %>
				<br>
				<%= link_to 'TestSuites','test_suites' %>
				<br>
				<%= link_to 'Tests','test_runs' %>
				<br>
				<br>
				<b>Admin</b>
				<br>
				<%= link_to 'Modules','product_modules' %>
				<br>
				<%= link_to 'States','deploy_states' %>
			</div>
			<div class="rechts">
				<b>Actions</b>
				<br>
				Test okay!
				<br>
				Edit
			</div>
			<div>
				<section class="round">
					<% flash.each do |key, value| %>
					<div class="flash <%= key %>">
						<%= value %>
					</div>
					<% end %>
					<%= yield %>
				</section>
			</div>
			<%= render 'layouts/footer' %>
			<%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>
